  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File with args",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": [
          "-j", "journal", "-d", "-t", "2021-05-05T124017"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Quart",
      "type": "python",
      "request": "launch",
      "module": "quart",
      "env": {
        "QUART_APP": "app.py",
        "QUART_ENV": "development",
        "QUART_DEBUG": "0"
      },
      "args": ["run"],
      "jinja": true
    }
  ]
}
